datasource db {
    provider = "postgresql"
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id      Int    @id @default(autoincrement())
    name    String
    classId Int
    class   Class  @relation(fields: [classId], references: [id])
}

model Class {
    id    Int    @id @default(autoincrement())
    name  String
    users User[]
}
